    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0-beta10",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
