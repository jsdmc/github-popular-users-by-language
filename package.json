{
  "name": "github-popular-users-by-language",
  "version": "1.0.0",
  "description": "Shows most popular github users by selected programming language.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "npm run clean && node devServer.js",
    "lint": "eslint src",
    "pretest": "eslint src/test",
    "test": "cross-env NODE_ENV=test cross-env NODE_PATH=./src mocha ./src/test --recursive --compilers js:babel-core/register --require ./src/test/setup.js --require ignore-styles",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsdmc/github-popular-users-by-language.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform",
    "redux",
    "redux-saga"
  ],
  "author": "Dmytro Chepunryi",
  "bugs": {
    "url": "https://github.com/jsdmc/github-popular-users-by-language/issues"
  },
  "homepage": "https://github.com/jsdmc/github-popular-users-by-language",
  "dependencies": {
    "axios": "^0.9.1",
    "parse-link-header": "^0.4.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "2.0.0",
    "react-router-redux": "^3.0.0",
